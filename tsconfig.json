{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "lib": ["es2017", "dom"],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./",
    "paths": {
      "../*": ["node_modules/*"]
    },
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "allowJs": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}
